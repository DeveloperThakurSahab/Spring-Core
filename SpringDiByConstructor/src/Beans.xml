<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="s1" class="com.bean.Student">
	<constructor-arg name="id" value="1"/>
	<constructor-arg name="fname" value="Ankush"/>
	<constructor-arg name="lname" value="Rajput"/>
	<constructor-arg name="email" value="ankush@gmail.com"/>
</bean>

<bean id="s2" class="com.bean.Student">
	<constructor-arg name="id" value="2"/>
	<constructor-arg name="fname" value="Thakur"/>
	<constructor-arg name="lname" value="Shahab"/>
	<constructor-arg name="email" value="thakur@gmail.com"/>
</bean>

</beans>
