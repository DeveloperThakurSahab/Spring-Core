<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="s1" class="com.bean.Student" init-method="init" destroy-method="destroy">
	<property name="id" value="1" />
	<property name="fname" value="Ankush" />
	<property name="lname" value="Rajput" />
	<property name="email" value="ankush@gmail.com" />
</bean>

<bean id="s2" class="com.bean.Student" init-method="init" destroy-method="destroy">
	<property name="id" value="2" />
	<property name="fname" value="Thakur" />
	<property name="lname" value="Sahab" />
	<property name="email" value="thakur@gmail.com" />
</bean>

</beans>
